
<%= render 'books/common_book' ,user:@user,book:@book %>


<table>
	<thead>
		<tr>
			<th>image</th>
			<th>name</th>
			<th colspan = "1"></th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
			<tr>
				<td><%= image_tag('no_image.jpg') %></td>
				<td><%= user.name %></td>
				<td><%= link_to 'Show',user_path(user) %></td>
				
			</tr>
		<% end %>
	</tbody>
</table>